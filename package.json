{
  "name": "stephanie-hicks-yoga-dot-com",
  "version": "1.0.0",
  "description": "Website for Stephanie Hicks Yoga.",
  "private": true,
  "scripts": {
    "build": "npm run clean && npm run prep-data && gulp dist && eleventy",
    "prep-data": "babel-node ./_build/prep-data.js",
    "start": "concurrently \"npm run sass\" \"eleventy --watch \" \"live-server ./_site\" -n SASS,ELEVENTY,SERVE -c bgBlue,bgGreen,bgYellow",
    "sass": "gulp",
    "clean": "rimraf ./_site",
    "test": "jest",
    "test-watch": "jest --watchAll"
  },
  "keywords": [
    "Stephanie Hicks",
    "Yoga"
  ],
  "author": "Steven J Hicks",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.3.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "concurrently": "^3.5.1",
    "date-fns": "^1.29.0",
    "fs-extra": "^5.0.0",
    "gulp": "^3.9.1",
    "gulp-sass": "^3.1.0",
    "jest": "^22.4.3",
    "live-server": "^1.2.2",
    "lodash.sortby": "^4.7.0",
    "luxon": "^1.0.0",
    "moment": "^2.22.1",
    "rimraf": "^2.6.2"
  },
  "dependencies": {}
}
